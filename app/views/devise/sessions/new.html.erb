<div class="main">
<div class="container">
  <div class="row">
    <div class="col-lg-6 sign-up-left">
      <div class="phone">
        <div class="pictures">
          <div id="carouselExampleFade" class="carousel carousel-fade" data-bs-ride="carousel">
            <div class="carousel-inner">
              <div class="carousel-item active">
                <%= image_tag("sign_up/ss1.png") %>
              </div>
              <div class="carousel-item">
                <%= image_tag("sign_up/ss2.png") %>
              </div>
              <div class="carousel-item">
                <%= image_tag("sign_up/ss3.png") %>
              </div>
              <div class="carousel-item">
                <%= image_tag("sign_up/ss4.png") %>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>

    <div class="col-lg-6 sign-up-right">

      <div class="sign-up-box">

        <%= image_tag("sign_up/logo.png", width: 180) %>

        <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
        <div class="form-group">
        <%= f.email_field :email, autofocus: true, autocomplete: "email", placeholder: "Emails" %>
        </div>

        <div class="form-group">
        <%= f.password_field :password, autocomplete: "current-password", placeholder: "Password" %>
        </div>

        <div class="actions">
        <%= f.submit "Log in", class: "btn btn-primary log-in-btn" %>
        </div>
        <% end %>

        <%= render "devise/shared/links" %>
      </div>
    </div>
  </div>
</div>
</div>
